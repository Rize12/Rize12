local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "JUST CONFETTI",
	LoadingTitle = "JUST CONFETTI",
	LoadingSubtitle = "By Rize",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "sirius", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "LOL",
		Subtitle = "Key System",
		Note = "",
		FileName = "SiriusKey",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = ""
	}
})



local Tab = Window:CreateTab("Confetti autofarm", 4483362458) -- Title, Image
local Section = Tab:CreateSection("Confetti")
local Button = Tab:CreateButton({
	Name = "Enable",
	Callback = function()
	_G.AutoConfetti = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptpastebin/raw/main/Confetti"))()
		-- The function that takes place when the button is pressed
	end,
})
local Button = Tab:CreateButton({
	Name = "Disable",
	Callback = function()
	_G.AutoConfetti = false
loadstring(game:HttpGet(""))(https://raw.githubusercontent.com/scriptpastebin/raw/main/Confetti)
		-- The function that takes place when the button is pressed
	end,
})
